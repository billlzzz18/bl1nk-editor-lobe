    "reinstall": "pnpm run clean && pnpm install",
    "reinstall:all": "pnpm run clean:all && pnpm install",
    "services:dev": "concurrently \"pnpm --filter ./server/backend-python dev\" \"pnpm --filter ./server websocket\" \"pnpm --filter ./server/nodeserver dev\" \"pnpm dev\"",
